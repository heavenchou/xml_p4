    <xs:attributeGroup ref="ns1:tei.global.attribute.xmllang"/>
    <xs:attributeGroup ref="ns1:tei.global.attribute.rend"/>
    <xs:attributeGroup ref="ns1:tei.global.attribute.xmlbase"/>
