    <xs:attributeGroup ref="ns1:tei.global.attribute.rend"/>
    <xs:attributeGroup ref="ns1:tei.global.attribute.xmlbase"/>
    <xs:attributeGroup ref="ns1:tei.linking.attribute.corresp"/>
    <xs:attributeGroup ref="ns1:tei.linking.attribute.synch"/>
    <xs:attributeGroup ref="ns1:tei.linking.attribute.sameAs"/>
    <xs:attributeGroup ref="ns1:tei.linking.attribute.copyOf"/>
    <xs:attributeGroup ref="ns1:tei.linking.attribute.next"/>
    <xs:attributeGroup ref="ns1:tei.linking.attribute.prev"/>
    <xs:attributeGroup ref="ns1:tei.linking.attribute.exclude"/>
    <xs:attributeGroup ref="ns1:tei.linking.attribute.select"/>
